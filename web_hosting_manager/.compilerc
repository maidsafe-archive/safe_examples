{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.7.8" } }],
          "react",
          "stage-0"
        ],
        "plugins": [
          "add-module-exports",
          "transform-async-to-generator",
          "transform-object-rest-spread",
          "transform-class-properties",
          "transform-es2015-classes",
          "react-hot-loader/babel"
        ],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.7.8" } }],
          "react",
          "stage-0"
        ],
        "plugins": [
          "add-module-exports",
          "transform-async-to-generator",
          "transform-object-rest-spread",
          "transform-class-properties",
          "transform-es2015-classes",
          "react-hot-loader/babel"
        ],
        "sourceMaps": "none"
      }
    }
  }
}
