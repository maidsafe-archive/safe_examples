{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.6.0" } }],
          "react",
          "stage-0",
          "es2017-node7"
        ],
        "plugins": [
          "add-module-exports", 
          "transform-async-to-generator", 
          "transform-object-rest-spread", 
          "transform-class-properties", 
          "transform-es2015-classes", 
          "react-hot-loader/babel",
          "react-hot-loader/babel",
          "transform-async-to-generator"
        ],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.6.0" } }],
          "react",
          "stage-0",
          "es2017-node7"
        ],
        "plugins": [
          "add-module-exports", 
          "transform-async-to-generator", 
          "transform-object-rest-spread", 
          "transform-class-properties", 
          "transform-es2015-classes",
          "react-hot-loader/babel",
          "transform-async-to-generator"
        ],
        "sourceMaps": "none"
      }
    }
  }
}
