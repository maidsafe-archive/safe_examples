{
"application/javascript": {
  "presets": [
    ["env", { "targets": { "electron": "1.6.0" } }],
    "es2015",
    "stage-0",
    "babel-polyfill",
    "react"
  ],
  "sourceMaps": "none",
  "env": {
    "development": {
        "plugins": [
          "transform-async-to-generator",
          "transform-class-properties",
          "transform-es2015-classes",
          "react-hot-loader/babel"
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "transform-async-to-generator",
          "transform-class-properties",
          "transform-es2015-classes",
          "react-optimize"
        ]
      }
    }
  },
  "text/less": {
    "dumpLineNumbers": "comments"
  }
}
